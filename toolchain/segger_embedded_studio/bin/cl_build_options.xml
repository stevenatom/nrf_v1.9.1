<!DOCTYPE CrossStudio_Project_Options>
<options>  
  <property tool="compiler" name="cl_warning_level" type="Enumeration" group="Build Options" short="Warning Level" long="Warning level" enumeration="Off (WO);Level 1 (W1);Level 2 (W2);Level 3 (W3);Level 4 (W4)" />

  <property tool="compiler" name="cl_optimization_level" type="Enumeration" group="Compiler Options (Optimization)" short="Optimization" long="Code optimization" enumeration="Disabled (Od);Minimize Size (O1);Maximize Speed (O2);Full Optimization (Ox);Custom" />
  <property tool="compiler" name="cl_optimization_no_alias" type="Enumeration" group="Compiler Options (Optimization)" short="No Aliasing" long="Assume no aliasing" enumeration="Yes (Oa);No" default="No"/>
  <property tool="compiler" name="cl_optimization_no_alias_within_functions" type="Enumeration" group="Compiler Options (Optimization)" short="No Aliasing Within Functions" long="Assume no aliasing within functions" enumeration="Yes (Ow);No" default="No"/>
  <property tool="compiler" name="cl_optimization_inline_function_expansion" type="Enumeration" group="Compiler Options (Optimization)" short="Inline Function Expansion" long="Controls the inlining of functions" enumeration="Disabled (Ob0);Inline (Ob1);Auto-inline (Ob2)" />  
  <property tool="compiler" name="cl_optimization_global_optimizations" type="Enumeration" group="Compiler Options (Optimization)" short="Global Optimizations" long="Enables global optimizations" enumeration="Yes (Og);No" default="No"/>  
  <property tool="compiler" name="cl_optimization_generate_intrinsic_functions" type="Enumeration" group="Compiler Options (Optimization)" short="Generate Intrinsic Functions" long="Enables generation of intrinsic functions" enumeration="Yes (Oi);No" default="No"/>    
  <property tool="compiler" name="cl_optimization_improve_float_consistency" type="Enumeration" group="Compiler Options (Optimization)" short="Improve Float Consistency" long="Improve the consistency of test for equality and inequality" enumeration="Improve (Op);Default" default="Default"/>      
  <property tool="compiler" name="cl_optimization_favor_size_speed_code" type="Enumeration" group="Compiler Options (Optimization)" short="Favor Size or Speed" long="Favor size or speed" enumeration="Favor Size (Os);Favor Speed (Ot);Neither" default="Neither"/>        
  <property tool="compiler" name="cl_optimization_omit_frame_pointer" type="Enumeration" group="Compiler Options (Optimization)" short="Omit Frame Pointer" long="Suppress creation of frame pointers" enumeration="Yes (Oy);No" default="No"/>        
  <property tool="compiler" name="cl_optimization_enable_fiber_safety" type="Enumeration" group="Compiler Options (Optimization)" short="Support Fiber-Safe TLS" long="Supports fiber safety for thread local storage" enumeration="Yes (GT);No" default="No"/> 
  <property tool="compiler" name="cl_optimization_optimize_for_processor" type="Enumeration" group="Compiler Options (Optimization)" short="Optimize For Processor" long="Optimize for the specified processor" enumeration="Blended (GB); Pentium (G5);Pentium Pro, Pentium II, Pentium III (G6);Pentium 4 and Above (G7)" /> 
  <property tool="compiler" name="cl_optimization_windows_application" type="Enumeration" group="Compiler Options (Optimization)" short="Optimize For Windows Application" long="Optimize code for .EXE rather than .DLL" enumeration="Yes (GA);No" default="No"/> 
  
  <property tool="compiler" name="cl_code_generation_enable_string_pooling" type="Enumeration" group="Compiler Options (Code Generation)" short="Enable String Pooling" long="Enable string pooling" enumeration="Yes (GF);No" default="No"/>        
  <property tool="compiler" name="cl_code_generation_enable_minimal_rebuild" type="Enumeration" group="Compiler Options (Code Generation)" short="Enable Minimal Rebuild" long="Enable minimal rebuild" enumeration="Yes (Gm);No" default="No"/>        
  <property tool="compiler" name="cl_code_generation_enable_smaller_type_check" type="Enumeration" group="Compiler Options (Code Generation)" short="Run-Time Smaller Type Check" long="Smaller type check" enumeration="Yes (RTCc);No" default="No"/>        
  <property tool="compiler" name="cl_code_generation_basic_runtime_checks" type="Enumeration" group="Compiler Options (Code Generation)" short="Run-Time Error Check" long="Run-Time error check" enumeration="Stack Frames (RTCs);Uninitialized Variables (RTCu);Both (RTC1);Default" default="Default"/>        
  <property tool="compiler" name="cl_code_generation_runtime_library" type="Enumeration" group="Compiler Options (Code Generation)" short="Run-Time Library" long="Run-Time library selection" enumeration="Multi-threaded (MT);Multi-threaded Debug (MTd);Multi-threaded DLL (MD);Multi-threaded Debug DLL (MDd);Single-threaded (ML);Single-threaded Debug (MLd)" /> 
  <property tool="compiler" name="cl_code_generation_struct_member_alignment" type="Enumeration" group="Compiler Options (Code Generation)" short="Struct Member Alignment" long="Struct member alignment" enumeration="1 Byte (Zp1);2 Bytes (Zp2);4 Bytes (Zp4);8 Bytes (Zp8);16 Bytes (Zp16);Default" default="Default"/>
  <property tool="compiler" name="cl_code_generation_buffer_security_check" type="Enumeration" group="Compiler Options (Code Generation)" short="Buffer Security Check" long="Check for buffer overruns" enumeration="Yes (GS);No" default="No"/>
  <property tool="compiler" name="cl_code_generation_enable_function_level_linking" type="Enumeration" group="Compiler Options (Code Generation)" short="Enable Function-Level Linking" long="Enables function-level linking" enumeration="Yes (Gy);No" default="No"/>
  <property tool="compiler" name="cl_code_generation_enable_enhanced_instruction_set" type="Enumeration" group="Compiler Options (Code Generation)" short="Enable Enhanced Instruction Set" long="Enable enhanced instruction set" enumeration="SSE (arch:SSE);SSE2 (arch:SSE2);Not Set" default="Not Set"/>
  <property tool="compiler" name="cl_code_generation_calling_convention" type="Enumeration" group="Compiler Options (Code Generation)" short="Call Convention" long="Select the calling convention" enumeration="__cdecl (Gd);__fastcall (Gr);__stdcall (Gz)" />
  <property tool="compiler" name="cl_code_generation_enable_big_objects" type="Enumeration" group="Compiler Options (Code Generation)" short="Enable Big Objects" long="Enable increased number of sections in object file" enumeration="Yes (bigobj);No" default="Yes (bigobj)"/>

  <property tool="compiler" name="cl_language_default_char_unsigned" type="Enumeration" group="Compiler Options (Language)" short="Default Char Unsigned" long="Set default char type to be unsigned" enumeration="Yes (J);No" default="No"/>
  <property tool="compiler" name="cl_language_wchar_t_is_native_type" type="Enumeration" group="Compiler Options (Language)" short="wchar_t Is Native Type" long="wchar_t is native type" enumeration="Yes (Zc:wchar_t);No" default="No"/>
  <property tool="compiler" name="cl_language_forscope" type="Enumeration" group="Compiler Options (Language)" short="Force Conformance In For Loop Scope" long="Standard C++ behaviour for initializers in for loops" enumeration="Yes (Zc:forScope);No" default="No"/>

  <property tool="linker" name="link_base" type="String" group="Linker Options" short="Base Address" long="Set base address for the program" specialvalue="base:"/>
  <property tool="linker" name="link_def" type="ProjFileName" group="Linker Options" short="Module-Definition File" long="The name of the module-defintion file" specialvalue="def:"/>  
  <property tool="linker" name="link_delayload" type="String" group="Linker Options" short="Delay Load" long="The name of the DLL you want to delay load" specialvalue="delayload:"/>    
  <property tool="linker" name="link_dll" type="Enumeration" group="Linker Options" short="Create A DLL" long="Creates a DLL" enumeration="Yes (dll);No" default="No"/>
  <property tool="linker" name="link_driver" type="Enumeration" group="Linker Options" short="Create A Driver" long="Creates a Windows NT kernel mode driver" enumeration="Yes (driver);No" default="No"/>
  <property tool="linker" name="link_entry_point" type="String" group="Linker Options" short="Entry-Point Symbol" long="Starting address for the program" specialvalue="entry:"/>
  <property tool="linker" name="link_import_library" type="ProjFileName" group="Linker Options" short="Import Library" long="The name of the import library file" specialvalue="implib:"/>  
  <property tool="linker" name="link_incremental" type="Enumeration" group="Linker Options" short="Incremental" long="Enable incremental linking" enumeration="Yes;No (incremental:no)" default="Yes"/>
  <property tool="linker" name="link_libpath" type="StringList" group="Linker Options" short="Additional Libary Path" long="Add directory to the library search path" specialvalue="libpath:"/>
  <property tool="linker" name="link_merge" type="String" group="Linker Options" short="Combine Sections" long="Combine sections from=to" specialvalue="merge:"/>
  <property tool="linker" name="link_ignore_all_default_libraries" type="Enumeration" group="Linker Options" short="Ignore All Default Libraries" long="Ignore all default libraries during linking" enumeration="Yes (nodefaultlib);No" default="No"/>
  <property tool="linker" name="link_ignore_specific_library" type="StringList" group="Linker Options" short="Ignore Specific Library" long="Ignore specific libraries during linking" specialvalue="nodefaultlib:"/>
  <property tool="linker" name="link_subsystem" type="Enumeration" group="Linker Options" short="Subsystem" long="Specify subsystem" enumeration="Not Set;Native (subsystem:native);Console (subsystem:console);Windows (subsystem:windows)" default="Not Set"/>
</options>
